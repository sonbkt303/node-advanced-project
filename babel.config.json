{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@flamingo": "./src",
          "@datasource": "./src/datasource",
          "@config": "./src/config",
          "@directives": "./src/schema/directive"
        }
      }
    ]
  ]
}

{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage", // @babel/polyfill:
        "corejs": "3.6.5"
      }
    ],
  ],
  "compact": true,
  "plugins": ["@babel/plugin-transform-runtime","@babel/plugin-proposal-export-default-from"]
}
